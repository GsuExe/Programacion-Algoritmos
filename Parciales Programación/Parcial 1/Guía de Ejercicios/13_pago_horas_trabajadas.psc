Proceso pago_horas_trabajadas
	Definir num_horas,costo_hora Como Entero;
	
	Escribir "Ingrese el número de horas trabajadas: ";
	Leer num_horas;
	
	Escribir "Ingrese el costo por hora trabajada: ";
	Leer costo_hora;
	
	Escribir "El pago que le corresponde es de: $",num_horas * costo_hora;
	
	
FinProceso
