Proceso imprimir_salario
	Definir horas,tarifa,salario Como Entero;
	
	Escribir "Ingrese la cantidad de horas trabajadas: ";
	Leer horas;
	
	Escribir "Ingrese la tarifa de pago por hora: ";
	Leer tarifa;
	
	salario = horas * tarifa;
	
	Escribir "Su salario corresponde a: $",salario;

FinProceso